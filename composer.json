{
  "name": "bugover/ship-manager",
  "type": "package",
  "version": "1.0.7",
  "description": "Nuclear Ship folder where shared functionality resides",
  "license": "MIT",
  "require": {
    "php": "<=8.2.3",
    "php-64bit": "<=8.2.3",
    "bugover/ship": "^1.0.0",
    "composer/installers": "^2.0.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "barryvdh/laravel-ide-helper": "^3.0.0",
    "barryvdh/laravel-debugbar": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Ship\\": ""
    }
  },
  "extra": {
    "installer-name": "Ship"
  },
  "scripts": {
    "post-update-cmd": []
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
